#include <bits/stdc++.h>

#define MOD 1000000007
#define PI 3.1415926535897932384626433832795

using namespace std;

void solve() {
  int w1, w2;
  cin >> w1;
  cin >> w2;
  int count = 0;
  while (w1 <= w2) {
    w1 *= 3;
    w2 *= 2;
    count++;
  }
  cout << count << '\n';
}

int main() {
  ios::sync_with_stdio(0);
  cin.tie(0);

  // LOGIC

  solve();

  return 0;
}
