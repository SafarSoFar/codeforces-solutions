#include <bits/stdc++.h>

#define MOD 1000000007
#define PI 3.1415926535897932384626433832795

using namespace std;

void solve() {
  int tc;
  cin >> tc;
  while (tc-- > 0) {
    int n;
    cin >> n;
    int min = n / 4;
    int rem = n % 4;
    min += rem / 2;
    cout << min << '\n';
  }
}

int main() {
  ios::sync_with_stdio(0);
  cin.tie(0);

  // LOGIC

  solve();

  return 0;
}
